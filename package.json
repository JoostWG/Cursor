{
    "version": "10.1.0",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "migrate": "node ./dist/migrate.js --up",
        "rollback": "node ./dist/migrate.js --down",
        "watch": "tsc --watch --noEmit",
        "build": "tsc --build",
        "lint": "eslint . --fix --max-warnings=0 --cache",
        "format": "dprint fmt",
        "type-check": "tsc --noEmit",
        "spell-check": "cspell",
        "quality": "concurrently -n ESLint,dprint,TypeScript,CSpell -c #3178C6,#4D5564,#4B32C3,#B51A00 \"npm run lint\" \"npm run format\" \"npm run type-check\" \"npm run spell-check\""
    },
    "dependencies": {
        "axios": "^1.13.2",
        "better-sqlite3": "^12.4.6",
        "canvas": "^3.2.0",
        "chess.js": "^1.4.0",
        "discord.js": "^14.25.1",
        "fast-equals": "^5.3.3",
        "jolpica-f1-api": "github:JoostWG/jolpica-f1-api#build",
        "kysely": "^0.28.8",
        "lodash": "^4.17.21",
        "valicheck": "^0.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@stylistic/eslint-plugin": "^5.6.1",
        "@types/better-sqlite3": "^7.6.13",
        "@types/lodash": "^4.17.21",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "concurrently": "^9.2.1",
        "cspell": "^9.4.0",
        "dprint": "^0.50.2",
        "eslint": "^9.39.1",
        "eslint-plugin-check-file": "^3.3.1",
        "globals": "^16.5.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.47.0"
    }
}
