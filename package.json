{
    "version": "9.0.0",
    "scripts": {
        "migrate": "node ./dist/migrate.js --up",
        "rollback": "node ./dist/migrate.js --down",
        "watch": "tsc --watch --sourceMap",
        "build": "tsc --build --sourceMap",
        "lint": "eslint . --fix",
        "format": "dprint fmt",
        "type-check": "tsc --noEmit",
        "quality": "concurrently -n ESLint,dprint,TypeScript -c #3178C6,#4D5564,#4B32C3 \"npm run lint\" \"npm run format\" \"npm run type-check\" "
    },
    "dependencies": {
        "axios": "^1.12.2",
        "better-sqlite3": "^12.2.0",
        "canvas": "^3.2.0",
        "chess.js": "^1.4.0",
        "discord.js": "^14.22.1",
        "fast-equals": "^5.2.2",
        "jolpica-f1-api": "github:JoostWG/jolpica-f1-api",
        "kysely": "^0.28.7",
        "open-trivia-db": "^2.1.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@stylistic/eslint-plugin": "^5.3.1",
        "@types/better-sqlite3": "^7.6.13",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "concurrently": "^9.2.1",
        "dprint": "^0.50.2",
        "eslint": "^9.26.0",
        "eslint-plugin-check-file": "^3.3.0",
        "globals": "^16.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.0"
    }
}
